{
  "manifest_version": 3,
  "name": "Dev Mode Alert",
  "version": "1.0.1",
  "description": "開発モードでない場合に警告を表示します",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://*.figma.com/*"],
      "js": ["main.js"],
      "css": ["styles.css"]
    }
  ],
  "host_permissions": ["*://*.figma.com/*"]
}
