{
  "manifest_version": 3,
  "name": "Non-Dev Mode Alert",
  "version": "1.0.0",
  "description": "Displays a persistent alert when not in dev mode",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://*.figma.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "host_permissions": ["*://*.figma.com/*"]
}
