{
  "manifest_version": 3,
  "name": "Non-Dev Mode Alert",
  "version": "1.0.4",
  "description": "Figmaを非Dev Modeで閲覧していると画面上に警告を表示するChrome拡張機能です",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.figma.com/*"],
      "js": ["main.js"],
      "css": ["styles.css"]
    }
  ]
}
